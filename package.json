{
    "devDependencies": {
        "@types/node": "^18.16.2",
        "@typescript-eslint/eslint-plugin": "^5.59.2",
        "copyfiles": "^2.4.1",
        "prettier": "2.8.8",
        "ts-node": "^10.9.1",
        "tsconfig-paths": "^4.2.0",
        "tscpaths": "^0.0.9",
        "tslib": "^2.5.3",
        "typescript": "^4.9.5"
    },
    "name": "anify-module",
    "description": "Anify module",
    "version": "1.0.0",
    "scripts": {
        "eslint": "eslint --ext .js --fix",
        "prettier": "prettier --write .",
        "lint": "npm run prettier && npm run eslint",
        "copy-files": "copyfiles -u 1 src/**/*.json dist/",
        "clean": "rimraf dist/",
        "build:ts": "tsc --project tsconfig.json && tscpaths -p tsconfig.json -s ./src -o ./dist && npm run copy-files"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Eltik/anify-module.git"
    },
    "author": "eltik",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/Eltik/anify-module/issues"
    },
    "homepage": "https://github.com/Eltik/anify-module#readme"
}
